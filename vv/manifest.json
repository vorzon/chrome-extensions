
{
	"manifest_version": 2,
	"name": "vip视频解析",
	"version": "1.0.1",
	"description": "解析各大视频网站的vip视频",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png",
			"128": "images/icon128.png"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	]
}